        <activity
            android:name=".Siren"
            android:theme="@android:style/Theme.DeviceDefault.Light.Dialog">
        </activity>
        <activity android:name=".Siren_select"/>
        <activity android:name=".Emergencycall"
            android:theme="@android:style/Theme.DeviceDefault.Light.Dialog">
